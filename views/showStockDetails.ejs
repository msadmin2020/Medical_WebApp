<% for(var i=0; i<stockDetails.length; i++) { %>
<tr>
    <td><%= stockDetails[i].item_name %></td>
    <td><%= stockDetails[i].item_detail %></td>
    <td><%= stockDetails[i].item_price %></td>
    <td><%= stockDetails[i].quantity %></td>
    <td><%= stockDetails[i].company_name %></td>
    <td><%= stockDetails[i].location %></td>
    <td><%= stockDetails[i].expire_date.split("T")[0] %></td>
    <td>
        <a href="<%= `/route/api/medical/stocks/${stockDetails[i]._id}/edit` %>" type="button" class="btn btn-success">Edit</a>
        <a href="<%= `/route/api/medical/stocks/${stockDetails[i]._id}?_method=DELETE` %>" type="button" class="btn btn-success"  onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
    </td>
</tr>
<% } %>